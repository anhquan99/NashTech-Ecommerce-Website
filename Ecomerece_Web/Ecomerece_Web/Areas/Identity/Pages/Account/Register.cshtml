@page
@model Ecomerece_Web.Areas.Identity.Pages.Account.RegisterModel
@{
    ViewData["Title"] = "Register";
}
<div class="PageLayout_PageAligment">
    <div class="Hero_Wrapper LoginView_Hero">
        <div class="Hero_FirstPane">
            <div class="Hero_HeroPane">
                <div class="Hero_HeroPaneContent LoginView_HeroPaneContent">
                    <img alt="Red Jordan 1s" src="~/image/login.jpg"
                         class="LoginView_Jordans">
                </div>
            </div>
        </div>
        <div class="Hero_SecondPane">
            <form class="RegisterForm_Form" method="post" asp-route-returnUrl="@Model.ReturnUrl" >
                <div class="Hero_HeroPane RegisterForm_HeroPane">
                    <div class="Hero_HeroPaneContent RegisterForm_HeroPaneContent">
                        <header class="RegisterForm_Header">
                            <h1 class="RegisterForm_Title">Sign Up</h1>
                        </header>
                        <main class="RegisterForm_Main">
                            <div class="MaterialInput_Content">
                                <label class="MaterialInput_Label">
                                    <input asp-for="Input.fullName" class="MaterialInput_Input"><span class="MaterialInput_LabelCopy">
                                        Full
                                        Name
                                    </span>
                                    <span asp-validation-for="Input.fullName" class="text-danger"></span>
                                </label>
                            </div>
                            <div class="MaterialInput_Content">
                                <label class="MaterialInput_Label">
                                    <input asp-for="Input.address" class="MaterialInput_Input"><span class="MaterialInput_LabelCopy">
                                        Address
                                    </span>
                                    <span asp-validation-for="Input.fullName" class="text-danger"></span>
                                </label>
                            </div>
                            <div class="MaterialInput_Content">
                                <label class="MaterialInput_Label">
                                    <input asp-for="Input.Email"
                                           class="MaterialInput_Input"><span class="
                                            MaterialInput_LabelCopy">Email</span>
                                    <span asp-validation-for="Input.Email" class="text-danger"></span>

                                </label>
                            </div>
                            <div class="MaterialInput_Content">
                                <label class="MaterialInput_Label">
                                    <input asp-for="Input.Password"
                                           class="MaterialInput_Input"><span class="
                                            MaterialInput_LabelCopy">Password</span>
                                    <span asp-validation-for="Input.Password" class="text-danger"></span>
                                </label>
                            </div>
                            <div class="MaterialInput_Content">
                                <label class="MaterialInput_Label">
                                    <input asp-for="Input.ConfirmPassword" class="MaterialInput_Input"><span class="
                                            MaterialInput_LabelCopy">
                                        Confirm
                                        Password
                                    </span>
                                    <span asp-validation-for="Input.ConfirmPassword" class="text-danger"></span>
                                </label>
                            </div>

                            <div class="RegisterForm_Terms">
                                <span>
                                    <span>
                                        By creating an account you
                                        agree to our
                                    </span> <a data-qa="terms_link" href="#">Terms</a> <span>and</span>
                                    <a data-qa="privacy_link" href="#">Privacy</a>
                                </span>
                            </div>
                        </main>
                    </div>
                    <nav class="Hero_HeroPaneLoginActions">
                        <a class="Hero_Link" href="~/Identity/Account/Login">
                            <button
                                    type="button" class="GoatButton_Wrapper Hero_Button">
                                Back
                            </button>
                        </a><span class="Hero_Border">
                            <button type="submit" class="GoatButton_Wrapper Hero_Button">
                                Sign
                                Up
                            </button>
                        </span>
                    </nav>
                </div>
            </form>


        </div>
    </div>
</div>

@section Scripts {
    <partial name="_ValidationScriptsPartial" />
}